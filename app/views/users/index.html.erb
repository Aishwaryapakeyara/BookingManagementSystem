<table>
<thead>
<th>Name</th>
<th>Phone</th>
<th>Email</th>
<th>Role</th>
<th>Book!</th>
</thead>
<tbody>
<% @user.each do |user| %>
<tr>
<td><%= user.name %></td>
<td><%= user.phone %></td>
<td><%= user.email %></td>
<td><%= user.roles.first.name%></td>
<td><%= link_to "Book", new_user_booking_path(user) %></td>
</tr>
<% end %>
</tbody>
</table>

<%= link_to "Add New User",  new_user_path %>